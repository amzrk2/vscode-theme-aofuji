{
  "name": "vscode-theme-aofuji",
  "displayName": "Aofuji Light Theme",
  "description": "A clean light theme extended from Aofuji.",
  "version": "0.2.0",
  "author": "DSRKafuU <amzrk2@outlook.com> (https://amzrk2.cc/)",
  "publisher": "amzrk2",
  "engines": {
    "vscode": "^1.31.1"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "vscode",
    "vscode-theme",
    "theme",
    "color-theme",
    "light",
    "light-theme"
  ],
  "icon": "src/icon.png",
  "galleryBanner": {
    "color": "#8aa2d3",
    "theme": "dark"
  },
  "contributes": {
    "themes": [
      {
        "label": "Aofuji",
        "uiTheme": "vs",
        "path": "./themes/aofuji-color-theme.json",
        "settings": {
          "color": "#8aa2d3",
          "italic": true
        }
      },
      {
        "label": "Oranji",
        "uiTheme": "vs",
        "path": "./themes/oranji-color-theme.json",
        "settings": {
          "color": "#ff9940",
          "italic": true
        }
      },
      {
        "label": "Aofuji (No Italic)",
        "uiTheme": "vs",
        "path": "./themes/aofuji-nl-color-theme.json",
        "settings": {
          "color": "#8aa2d3",
          "italic": false
        }
      },
      {
        "label": "Oranji (No Italic)",
        "uiTheme": "vs",
        "path": "./themes/oranji-nl-color-theme.json",
        "settings": {
          "color": "#ff9940",
          "italic": false
        }
      }
    ]
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/amzrk2/vscode-theme-aofuji",
  "bugs": {
    "url": "https://github.com/amzrk2/vscode-theme-aofuji/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amzrk2/vscode-theme-aofuji.git"
  },
  "devDependencies": {
    "json5": "^2.1.3",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "vsce": "^1.83.0"
  },
  "scripts": {
    "dev": "nodemon --watch themes/raw-color-theme.json --watch package.json --watch src --ext json,js scripts/build.js",
    "compile": "node scripts/build.js",
    "package": "vsce package",
    "build": "npm-run-all compile package"
  }
}
